#include <iostream>
using  namespace std;
int main()
{
	cout<<"I"<<endl<<'\n'<<"L"<<endl<<"o"<<endl<<"v"<<endl<<"e"<<endl<<'\n'<<"Y"<<endl<<"o"<<endl<<"u"<<endl<<'\n'<<"G"<<endl<<"P"<<endl<<"L"<<endl<<"T";
	return 0;
}



#include <iostream>
using  namespace std;
int main()
{   cout<<"--------------------------------------"<<endl;
	cout<<"Province      Area(km2)      Pop.(10K)"<<endl;
    cout<<"--------------------------------------"<<endl;
	cout<<"Anhui         139600.00        6461.00"<<endl;
    cout<<"Beijing       16410.54         1180.70"<<endl;
    cout<<"Chongqing     82400.00         3144.23"<<endl;
    cout<<"Shanghai      6340.50          1360.26"<<endl;
    cout<<"Zhejiang      101800.00        4894.00"<<endl;
        
	return 0;
}

#include <iostream>
using namespace std;
int main (){
	cout<<"计算i+=j+=2的结果"<<endl; 
	int i, j;
	i=0, j=0;
	i+=j+=2 ;
	cout<<"i="<< i<<' '<<"j="<<j;	
} 


 #include <iostream>
using  namespace std;
int main()
{
	char  c1,c2,c3,c4,c5;
   cin>>c1>>c2>>c3>>c4>>c5 ;
   
	c1=c1+4;
	c2=c2+4;
	c3=c3+4;
	c4=c4+4;
	c5=c5+4;
	cout<<c1<<c2<<c3<<c4<<c5;
	return 0;
}
 
 #include <iostream>
using  namespace std;
int main()
{
   char c;
   while (cin>>c){
  
   
   c+=4;
   cout<<c; }
	return 0;
}
 
 
#include <iostream>  //这一段是用了偷懒的方法写的
#include <iomanip>
#include <stdio.h>
using namespace std;
int main (){
	
	char ch='b';
	int i=3,j=5;
    float x=22.354, Y=435.6789;
	cout<<"ch="<<"'b'"<< ',' << "ASCII=98" << endl;
	cout<<"i="<<i<<"      "<<"j="<<j<<endl;
	cout<<setprecision(2)<<std::fixed<<"x="<<x<<"   "<<"Y="<<Y;
	return 0;
} 
#include <iostream>
#include <iomanip>
#include <stdio.h>
using namespace std;
int main (){
	
	char ch='b';
	int i=3,j=5;
    float x=22.354, Y=435.6789;
    int a;
    a=(int)ch;
	cout<<"ch="<<"'b'"<< ',' << "ASCII=" <<a<< endl;
	cout<<"i="<<i<<"      "<<"j="<<j<<endl;
	cout<<setprecision(2)<<std::fixed<<"x="<<x<<"   "<<"Y="<<Y;
	return 0;
} 



