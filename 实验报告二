#include <iostream>
using namespace std;
int main (){
	cout<<"计算i+=j+=2的结果"<<endl; 
	int i, j;
	i=0, j=0;
	i+=j+=2 ;
	cout<<"i="<< i<<' '<<"j="<<j;
	
} 


