#include <iostream>
using namespace std;
int main (){
	int N,n;
	cout<<"请输入一个100到1000之间的正整数"<<endl;
	cin>>N;
	for (int i=1;i<=N;i++)
	 for (int j=1;j<=N;j++)
	  for (int k=1;k<=N;k++)
	if (i*1+j*2+k*5==N){
		cout<<"一元"<<i<<"个"<<" "<<"两元"<< j<< "个"<<" "<<"五元"<<k<<" "<<"个"<<endl; 
	n++;
	} 
	cout<<"总的组合数为："<<n<<endl;
}


#include <iostream>
using namespace std;
int main (){
	cout<<"请输入一个年份：";
	int a;
	cin>>a;
	if (a%4==0&&a%100!=0)
	cout<<"该年是闰年"<<endl;
	else 
	cout<<"该年不是闰年"<<endl;
	return 0;
	system("pause");
}

#include <iostream>
using namespace std;
int main(void)
{
    int a=8;
    for (int b=1;b<=4;b++)
    {
	    a--;
		for (int n=1;n<=7;n++)
	    {
		    if (n>=b&&n<=a)
	            cout<<"*";
	        else
	            cout<<" ";
        }
        cout<<'\n';
    }

    int x=4;
	for (int y=3;y>=1;y--) 
    {
	    x++;
	    for (int n=1;n<=7;n++)
	    {
		    if (n>=y&&n<=x)
	            cout<<"*";
	        else
	            cout<<" ";
		}
	    cout<<'\n';
    }
    return 0;
}
 
 
 #include <iostream>
using namespace std;
int main (){
	char a;
	int b=1,c=0,d=0,e=0,f=0;
	
	cin>>a;
	for(int i=1;i<=17;i++)
	{
	  cin.get(a);
	  if ('A'<=a&&a<='Z')
	  b++;
	  else if ('a'<=a&&a<='z')
	  c++;
	  else if (a==' ')
	  d++;
	  else if ('0'<=a&&a<='9')
	  e++;
	  else
	  f++;	
	}
	cout<<b<<" "<<c<<" "<<e<<" "<<d<<" "<<f<<endl;
	return 0;
}
